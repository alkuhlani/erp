<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ERPPU" transaction-type="JTA">
    <jta-data-source>java:/jboss/datasources/ERPDS</jta-data-source>
    <class>com.newage.erp.accounting.entities.Account</class>
    <class>com.newage.erp.accounting.entities.Currency</class>
    <class>com.newage.erp.accounting.entities.Transaction</class>
    <class>com.newage.erp.accounting.entities.TransactionDetail</class>
    <class>com.newage.erp.security.entities.SecurityGroup</class>
    <class>com.newage.erp.security.entities.SecurityPermission</class>
    <class>com.newage.erp.security.entities.SecurityUser</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
    </properties>
  </persistence-unit>
</persistence>
